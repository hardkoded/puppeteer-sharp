<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net10.0;net471</TargetFrameworks>
    <LangVersion>12</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(TargetFramework)' == 'net10.0'">
    <PublishAot>true</PublishAot>
    <!-- IL3000: Assembly.Location in PuppeteerSharp has fallback handling for single-file/AOT -->
    <NoWarn>$(NoWarn);IL3000</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\lib\PuppeteerSharp\PuppeteerSharp.csproj" />
  </ItemGroup>
</Project>
