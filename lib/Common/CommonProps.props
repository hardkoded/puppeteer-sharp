<Project>
  <PropertyGroup>
    <AssemblyPublicKey></AssemblyPublicKey>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\keys\keypair.snk</AssemblyOriginatorKeyFile>
    <PublicSign>false</PublicSign>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\keys\keypair.snk">
      <Link>keypair.snk</Link>
    </None>
  </ItemGroup>
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="dotnet format $(ProjectPath)" />
  </Target>
</Project>
